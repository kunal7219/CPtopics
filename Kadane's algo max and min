long long maxSubarraySum(int arr[], int n){
        long long sum=0,maxi =arr[0];
        for(int i=0;i<n;i++){
            sum += arr[i];
            maxi = max(maxi,sum);
            if(sum<0)sum=0;
        }
        return maxi;
    } 
   
    int minimumSum (vector<int>& arr, int n)
{
    long long sum=0,mini =arr[0];
        for(int i=0;i<n;i++){
            sum += arr[i];
            mini = min(mini,sum);
            if(sum>0)sum=0;
        }
        return mini;
}
