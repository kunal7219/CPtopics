bool find3Numbers(int A[], int n, int X)
    {
        sort(A,A+n);
        for(int i=0;i<n;i++){
            int t=X-A[i];
            int s=i+1,e=n-1;
            while(s<e){
                if(A[s]+A[e]==t){
                    return true;
                }
                else if(A[s]+A[e]>t){
                    e--;
                }
                else{
                    s++;
                }
            }
        }
        return false;
    }
